    "ng2-bootstrap": "^1.0.5",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.22",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "amd-optimize": "^0.6.1",
    "bower": "^1.7.7",
    "browserify": "^13.0.0",
    "concurrently": "^2.0.0",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
