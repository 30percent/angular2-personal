{
  "name": "til-angular2",
  "version": "0.0.1",
  "description": "A site used to keep track of snippets of knowledge.",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run typings install",
    "tsc": "tsc",
